# Configurações do Baserow
BASEROW_TOKEN=YOUR_BASEROW_TOKEN
BASEROW_BASE_URL=https://your-baserow.example.com

# IDs das Tabelas do Baserow (substitua pelos IDs reais das suas tabelas)
BASEROW_ACCESS_LOG_TABLE_ID=631
BASEROW_PROFILES_TABLE_ID=456
BASEROW_WEBHOOKS_TABLE_ID=789

# Configurações do Google Drive (opcional)
# Use valores de ambiente locais; NÃO commit secrets reais.
GOOGLE_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_OAUTH_CLIENT_SECRET
GOOGLE_REDIRECT_URI=https://yourapp.example.com/oauth2callback

# Configurações dos Webhooks
WEBHOOK_POST_URL=https://your-webhook.example.com/post
WEBHOOK_GET_URL=https://your-webhook.example.com/get

# IP de Exceção (sem limitações de uso)
EXCEPTION_IP=

# Configurações do Telegram (para verificação de números)
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
TELEGRAM_STRING_SESSION=

# Provedor Fama24h (seguidores/visualizações)
FAMA24H_API_KEY=YOUR_FAMA24H_API_KEY

# Provedor GGRAM (curtidas brasileiras)
GGRAM_API_KEY=YOUR_GGRAM_API_KEY
# Se usar likes brasileiras fixas, defina o service ID correspondente
GGRAM_SERVICE_ID_LIKES_BRS=1810